		<menuitem id="menu_main_zdg_admin" name="Admin"
	      	sequence="41" groups="service.group_service_manager"/>
	      
	    <menuitem id="menu_admin_main" name="Administer"
	      	parent="menu_main_zdg_admin"/>
	      
	    <menuitem id="menu_admin_01" name="Temporary Tree 1"
	    	parent="menu_admin_main"
	    	action="action_admin_data_01"/>
	    	
	    <menuitem id="menu_admin_02" name="Temporary Tree 2"
	    	parent="menu_admin_main"
	    	action="action_admin_data_02"/>
	    	
	    <menuitem id="menu_admin_03" name="Temporary Tree 3"
	    	parent="menu_admin_main"
	    	action="action_admin_data_03"/>
