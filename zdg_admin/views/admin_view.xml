<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="view_admin_tree_01" model="ir.ui.view">
			<field name="name">service.other</field>
			<field name="model">service.other</field>
			<field name="arch" type="xml">
				<tree string="Admin">
					<field name="service_id"/>
					<field name="product_id"/>
					<field name="name"/>
					<field name="deductible"/>
				</tree>
			</field>
		</record>
		
		<record id="view_admin_tree_02" model="ir.ui.view">
			<field name="name">service.line</field>
			<field name="model">service.line</field>
			<field name="arch" type="xml">
				<tree string="Admin">
					<field name="service_id"/>
					<field name="product_id"/>
					<field name="name"/>
					<field name="cost_unit"/>
					<field name="cost_subtotal"/>
				</tree>
			</field>
		</record>
		
		<record id="view_admin_tree_03" model="ir.ui.view">
			<field name="name">account.invoice.line</field>
			<field name="model">account.invoice.line</field>
			<field name="arch" type="xml">
				<tree string="Admin">
					<field name="invoice_id"/>
					<field name="product_id"/>
					<field name="name"/>
					<field name="deductible"/>
				</tree>
			</field>
		</record>
		
		<record id="action_admin_data_01"
			model="ir.actions.act_window">
			<field name="name">Export Import</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">service.other</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree</field>
		</record>
		
		<record id="action_admin_data_02"
			model="ir.actions.act_window">
			<field name="name">Export Import 2</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">service.line</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree</field>
		</record>
		
		<record id="action_admin_data_03"
			model="ir.actions.act_window">
			<field name="name">Export Import 3</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">account.invoice.line</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree</field>
